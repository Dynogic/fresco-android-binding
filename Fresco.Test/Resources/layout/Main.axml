<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:fresco="http://schemas.android.com/apk/res-auto" android:orientation="vertical" android:layout_width="match_parent" android:layout_height="match_parent">
  <com.facebook.drawee.view.SimpleDraweeView
        android:id="@+id/basic_image"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:background="@android:color/holo_green_dark"/>
  <com.facebook.drawee.view.SimpleDraweeView
        android:id="@+id/webp_static"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:background="@android:color/holo_red_dark"/>
  <com.facebook.drawee.view.SimpleDraweeView
        android:id="@+id/webp_translucent"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:background="@android:color/holo_blue_bright"/>
  <com.facebook.drawee.view.SimpleDraweeView
        android:id="@+id/webp_animated"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:background="@android:color/holo_orange_dark"/>
  <com.facebook.drawee.view.SimpleDraweeView
        android:id="@+id/gif_animated"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:background="@android:color/black"/>
</LinearLayout>